package com.test;
class Dog{
	public Dog() {
		age = 1;
		color = "Black";
		System.out.println("Default Constructor Dog");
	}
	public Dog(int ag, String clr) {
		age = ag;
		color = clr;
	}
	String breed;
	int age;
	String color;
	void bakring( ) {};
	void hungry() {};
	void steeping() {};
}
