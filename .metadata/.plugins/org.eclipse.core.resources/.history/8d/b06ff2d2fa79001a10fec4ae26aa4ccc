// Instance variable (공용으로 사용)
class A{
	int data = 50;
}
// static variable
public class Test1 {
	// Local variable  메서드 내의 변수들이 로컬변수
	public static void main(String[] args) {
		String str = "1234";
		int a = Integer.parseInt(str);
		Integer a2 = new Integer(a); //Wrapper class
		String str2 = a2.toString(); //look up
		
	}

}
