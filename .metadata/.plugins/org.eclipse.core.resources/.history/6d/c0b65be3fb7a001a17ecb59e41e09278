
public class Test8 {

	public static void main(String[] args) {
		Dog d = new Dog();
		d.bark();
		d.breath();
		Animal a = d;
		a.breath();
		Dog d1 = (Dog)a;
		d1.bark();
		d1.breath();
		
		Animal a1 = new Dog();
		a1.breath();
		Animal a2 = new Cat();
		a2.breath();
	}
	
	static void Test1(Dog d) {}
	static void Test1(Cat c) {}
	
	//위에 하위 클래스 두개를 애니멀 클래스 하나로 뭉뚱그림
	static void Test1(Animal a) {}

}