package com.test2;
class A{
	void aMethod() {System.out.println("aMethod");}
	}
class B extends A{
	void bMethod() {System.out.println("bMethod");}
	void bMethod() {System.out.println(aMethod on B class);
	}
class C extends B{
	void cMethod() {System.out.println("cMethod");
	}
}
public class Test1 {
	public static void main(String[] args) {
		A a = new A();
		a.aMethod();
		
		B b = new B();
		b.aMethod();
		b.bMethod();
		
		C c = new C();
		c.aMethod();
		c.bMethod();
		c.cMethod();
	}

}
