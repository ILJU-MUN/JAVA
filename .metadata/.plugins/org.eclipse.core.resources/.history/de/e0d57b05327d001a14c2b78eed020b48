package com.test;
class A {
	int aValue;
	public A() {
		System.out.println("Default Constructor A");
		this.aValue = 10;
	}
}
class B extends A {
	int bValue;
	public B() {
		System.out.println("Default Constructor B");
		this.bValue = 20;
	}
	public B(int bValue) {
		this.bValue = bValue;
		System.out.println("Constructor B");
	}
}

public class Test6 {
	public static void main(String[] args) {
		//A a = new A();
		B b = new B();
		B b1 = new B(200);
	}

}
