
public class Test7 {

	public static void main(String[] args) {
		for(int i = 1; i < 6; i++) {
			System.out.println();
			for(int j = 0; j < i; j++) {
				System.out.print("*");
			}
		}
		int a=0;
		int b=0;
		while(a<6) {
			System.out.println();
			while(b<a) {
				System.out.print("*");
				break;
			}
		}
	}
}
