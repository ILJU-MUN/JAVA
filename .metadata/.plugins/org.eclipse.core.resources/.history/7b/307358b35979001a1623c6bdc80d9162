
public class Test3 {
		public static void main(String[] args) {
			int num1 = 100;
			int num2 = 40;
			System.out.println(num1);
			System.out.println(num2);
			int num3 = num1 + num2;
			System.out.println(num1 + "+" + num2 + "=" + num3 );
			System.out.println(num1 + "-" + num2 + "=" + num3);
		}
}
