
public class Test7 {

	public static void main(String[] args) {
		for(int i = 1; i < 6; i++) {
			System.out.println();
			for(int j = 0; j < i; j++) {
				System.out.print("*");
				for(int k = 5; k < 1; k--) {
					System.out.println();
					for(int l = 1; l < k; k--) {
						System.out.println("*");
					}
				}
			}
		}
	}
}
