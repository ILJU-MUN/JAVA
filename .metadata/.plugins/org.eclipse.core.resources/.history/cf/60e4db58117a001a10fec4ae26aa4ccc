
public class Test6 {

	public static void main(String[] args) {
		for(int i = 2; i < 10; i++) {
			System.out.println(i + "´Ü");
			for(int j = 1; j < 10; j++) {
				System.out.println(i + "X" + j + "=" + i * j);
			} 
			System.out.println();
		}
		
		for(int i2 = 9; i2 < 3; i2--) {
			System.out.println(i2 + "´Ü");
				for(int j2 = 1; j2 < 10; j++) {
					System.out.println(i + "X" + j + "=" + i * j);
				}
		}
	}
}
